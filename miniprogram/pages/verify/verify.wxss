.verify-container {
  padding-bottom: 48px;
}

.verify-wrapper {
  display: flex;
  justify-content: space-between;
  padding: 5px 10px;
  font-size: 14px;
}

.verify-item-info {
  display: flex;
  align-items: center;
}

.verify-item-info image {
  width: 50px;
  height: 50px;
  margin-right: 10px;
}

.verify-item-type {
  margin: auto 0;
}

.verify-item-type .iconfont {
  margin-left: 10px;
  color: #f3b41b;
}