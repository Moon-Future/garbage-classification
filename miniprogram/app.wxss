/**app.wxss**/
@import "/style/iconfont.wxss";
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.disable {
  color: #888;
}


.color-recyclable {
  color: #366aba;
}
.color-kitchen {
  color: #53c968;
}
.color-harmful {
  color: #e6222b;
}
.color-other {
  color: #5f5e5e;
}
